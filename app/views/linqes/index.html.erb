<div align="right"> <%= button_to "Submit New", new_linqe_path, method: :get, class:"btn btn-primary btn-sm"%></div>
<h1 align="center">Linqes</h1>


<% @linqes.each.with_index(1) do |linqe, i| %>
    <h4><%= "#{i}." %> <%= link_to "#{linqe.title}", linqe.url, target: "_blank"  %> </h3>
    <%= button_to "Delete Linq", linqe_path(linqe), method: :delete, class:"btn btn-secondary btn-sm"%>
  <% end %>
